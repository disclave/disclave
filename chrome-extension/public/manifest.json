{
  "name": "Disclave",
  "description": "Disclave Chrome extension",
  "version": "1.0.2",
  "manifest_version": 2,
  "permissions": ["activeTab"],
  "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}